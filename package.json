{
  "name": "mcpserversetup",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "test": "playwright test",
    "test:headed": "playwright test --headed",
    "test:chromium": "playwright test --project=chromium",
    "test:firefox": "playwright test --project=firefox",
    "test:webkit": "playwright test --project=webkit",
    "test:debug": "playwright test --debug",
    "test:list": "playwright test --list",
    "test:fixtures": "playwright test appointment-booking-fixtures.spec.ts",
    "test:parametrized": "playwright test parametrized-fixtures.spec.ts",
    "test:legacy": "playwright test gotjunk-appointment.spec.ts",
    "report": "playwright show-report",
    "install:browsers": "playwright install --with-deps",
    "ci:test": "playwright test --reporter=github",
    "ci:install": "npm ci && playwright install --with-deps"
  },
  "keywords": [
    "playwright",
    "testing",
    "fixtures",
    "automation",
    "e2e",
    "typescript"
  ],
  "author": "Yub Uddin",
  "license": "ISC",
  "description": "A comprehensive Playwright test automation project demonstrating various fixture patterns and best practices",
  "devDependencies": {
    "@playwright/test": "^1.53.0",
    "@types/node": "^24.0.3"
  }
}
